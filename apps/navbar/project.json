{
  "name": "navbar",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "app",
  "sourceRoot": "apps/navbar/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/angular:application",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/navbar",
        "index": "apps/navbar/src/index.html",
        "browser": "apps/navbar/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "apps/navbar/tsconfig.app.json",
        "assets": ["apps/navbar/src/assets"],
        "styles": [],
        "plugins": [],
        "externalDependencies": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "8kb",
              "maximumError": "10kb"
            }
          ],
          "outputHashing": "none"
        },
        "development": {
          "optimization": false,
          "extractLicenses": false,
          "sourceMap": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@nx/angular:dev-server",
      "options": {
        "port": 4203,
        "hmr": true
      },
      "configurations": {
        "production": {
          "buildTarget": "navbar:build:production"
        },
        "development": {
          "buildTarget": "navbar:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "navbar:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/navbar/jest.config.ts"
      }
    }
  }
}
